    "page6 = requests.get('https://www.journals.elsevier.com/artificial-intelligence/most-downloaded-articles')\n",
    "page6"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "id": "b2278f06",
   "metadata": {},
   "outputs": [],
   "source": [
    "soup6 = BeautifulSoup(page6.content).find('ul',attrs={'class':\"sc-9zxyh7-0 cMKaMj\"})\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 62,
   "id": "8a900d6a",
   "metadata": {},
   "outputs": [],
   "source": [
    "paper_title=[]\n",
    "authors=[]\n",
    "published_date=[]\n",
    "for x in soup6.find_all('li',{'class':\"sc-9zxyh7-1 sc-9zxyh7-2 kOEIEO hvoVxs\"}):\n",
    "    for y in x.find_all('h2',{'class':\"sc-1qrq3sd-1 gRGSUS sc-1nmom32-0 sc-1nmom32-1 btcbYu goSKRg\"}):\n",
    "        paper_title.append(y.text)\n",
    "    for z in x.find_all('span',{'class':\"sc-1w3fpd7-0 dnCnAO\"}):\n",
    "        authors.append(z.text)\n",
    "    for t in x.find_all('span',{'class':\"sc-1thf9ly-2 dvggWt\"}):\n",
    "        published_date.append(t.text)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 63,
   "id": "7afe9aa5",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "24"
      ]
     },
     "execution_count": 63,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "Paper_URL =[]\n",
    "for a in soup6.find_all('a',href=True):\n",
    "    href = a.get('href')\n",
    "    Paper_URL.append(href)\n",
    "len(Paper_URL)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 64,
   "id": "89cf9dd9",
