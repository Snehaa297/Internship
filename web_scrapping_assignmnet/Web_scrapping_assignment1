   "source": [
    "#printing all header tags\n",
    "heading_tags = [\"h1\", \"h2\", \"h3\"]\n",
    "\n",
    "for tags in soup1.find_all(heading_tags):\n",
    "    print(tags.name + ' -> ' + tags.text.strip())"
