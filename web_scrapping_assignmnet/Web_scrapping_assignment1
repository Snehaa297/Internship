   ],
   "source": [
    "# scraping the list of president names and there term_of_office using for loop \n",
    "import pandas as pd\n",
    "data = []\n",
    "for div in soup2:\n",
    "    Name = div.find(\"h3\").getText()\n",
    "    Term_of_Office = div.find(\"p\").text.strip(\"Term of Office:\") ## after extraction, striping off text Term of Office\n",
    "    data.append([Name,Term_of_Office])\n",
    "\n",
    "President = pd.DataFrame(data, columns=[\"Name\", \"Term_of_Office\"]) ## creating a dataframe and displaying the table.\n",
    "\n",
    "President"
   ]
  },
