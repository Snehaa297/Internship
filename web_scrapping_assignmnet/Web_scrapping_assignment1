   "cell_type": "code",
   "execution_count": 70,
   "id": "fa66dc80",
   "metadata": {},
   "outputs": [],
   "source": [
    "restaurant_name=[]\n",
    "cuisine=[]\n",
    "location=[]\n",
    "ratings=[]\n",
    "images =[]\n",
    "for x in soup7.find_all('div',{'class':\"restnt-card restaurant\"}):\n",
    "    for y in x.find_all('a',{'class':\"restnt-name ellipsis\"}):\n",
    "        restaurant_name.append(y.text)\n",
    "    for z in x.find_all('span',{'class':\"double-line-ellipsis\"}):\n",
    "        cuisine.append(z.text.split('|', 1)[1])\n",
    "    for t in x.find_all('div',{'class':\"restnt-loc ellipsis\"}):\n",
    "        location.append(t.text)\n",
    "    for r in x.find_all('div',{'class':\"restnt-rating rating-4\"}):\n",
    "        ratings.append(r.text)\n",
    "    for img in x.find_all('img',{'class':'no-img'}):\n",
    "        images.append(img['data-src'])"
   ]
  },
